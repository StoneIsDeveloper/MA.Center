<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="superSocket" 
             type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig,SuperSocket.SocketEngine"/>
    
  </configSections>
  <superSocket>
    <servers>
      <server name="ChatSocket" 
              textEncoding="gb2312" 
              serverType="ChatServer"
              ip="Any" 
              port="2018" 
              maxConnectionNumber="100">
      </server>
      <serverTypes>
        <add name="ChatServer"
             type="MA.SocketServer.AppServers.ChatServer,  MA.SocketServer"/>
      </serverTypes>
      
    </servers>
    
  </superSocket>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  
</configuration>